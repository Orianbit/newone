#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<"Enter the elementL:"<<endl;
    cin>>a>>b;
    char choice;
    cout<<"enter the choice:";
    cin>>choice;
    switch (choice)
    {
    case '+':
        cout<<"addition:"<<a+b;
        break;
    case '-':
        cout<<"subtract:"<<a-b;
        break;
    case '*':
		cout<<"multiply:"<<a*b;
        break;
    case '/':
        cout<<"divide:"<<a/b<<endl;
        break;
    default:
        cout<<"invalid choice:"<<endl;
        break;
    }

return 0;
}

